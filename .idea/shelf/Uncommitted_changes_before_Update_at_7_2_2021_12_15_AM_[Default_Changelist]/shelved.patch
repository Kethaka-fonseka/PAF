<+>import React, { Component } from \"react\";\r\nimport axios from \"axios\";\r\nimport DocViewer from \"react-doc-viewer\";\r\n\r\nclass ViewSubmission extends Component {\r\n  constructor(props) {\r\n    super(props);\r\n    this.state = {\r\n      submissions: [],\r\n    };\r\n    this.Approve = this.Approve.bind(this);\r\n    this.Decliend = this.Decliend.bind(this);\r\n  }\r\n\r\n  componentDidMount() {\r\n    axios\r\n      .get(`http://localhost:8080/submission/${this.props.match.params.id}`)\r\n      .then((res) => {\r\n        this.setState({ submissions: res.data });\r\n      })\r\n      .catch((err) => {\r\n        console.log(err);\r\n      });\r\n  }\r\n\r\n  Approve() {\r\n    axios\r\n      .patch(\r\n        `http://localhost:8080/submission/approve/${this.props.match.params.id}`\r\n      )\r\n      .then((res) => {\r\n        alert(\"Submission aprroved for the event\");\r\n      })\r\n      .catch((err) => {\r\n        console.log(err);\r\n      });\r\n  }\r\n\r\n  Decliend() {\r\n    axios\r\n      .patch(\r\n        `http://localhost:8080/submission/decline/${this.props.match.params.id}`\r\n      )\r\n      .then((res) => {\r\n        alert(\"Submission Declined for the event\");\r\n      })\r\n      .catch((err) => {\r\n        console.log(err);\r\n      });\r\n  }\r\n\r\n  render() {\r\n    return (\r\n      <div className=\"container mt-3 bg-secondary text-white rounded\">\r\n        <h1>Submission Details:</h1>\r\n\r\n        <h5>Subject : {this.state.submissions.subject}</h5>\r\n        <h5>Description:{this.state.submissions.description}</h5>\r\n        <h5>Submitted Date and Time : {this.state.submissions.date}</h5>\r\n        <div className=\"mt-3 mb-1\">\r\n          <a\r\n            class=\"btn btn-primary mx-2 rounded\"\r\n            href={`http://localhost:8080/submission/download/${this.props.match.params.id}`}\r\n            download\r\n          >\r\n            Download\r\n          </a>\r\n          <button\r\n            type=\"button\"\r\n            class=\"btn btn-success mx-2 rounded\"\r\n            onClick={this.Approve}\r\n          >\r\n            Approve\r\n          </button>\r\n          <button\r\n            type=\"button\"\r\n            class=\"btn btn-danger mx-2 rounded\"\r\n            onClick={this.Decliend}\r\n          >\r\n            Decline\r\n          </button>\r\n        </div>\r\n      </div>\r\n    );\r\n  }\r\n}\r\n\r\nexport default ViewSubmission;\r\n
